{
    "name": "drupal/drupal",
    "description": "Drupal is an open source content management platform powering millions of websites and applications.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.2",
	"wikimedia/composer-merge-plugin": "^2.0",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.8.8",
        "drupal/core-composer-scaffold": "^8.8.0",
        "drush/drush": "^9.7.1 | ^10.0.0",
        "vlucas/phpdotenv": "^4.0",
        "webflo/drupal-finder": "^1.0.0",
        "zaporylie/composer-drupal-optimizations": "^1.0",
        "drupal/twig_tweak": "^2.5",
        "drupal/devel": "^2.1",
        "drupal/bootstrap_barrio": "^4.25",
        "drupal/feeds_ex": "^1.0",
        "drupal/ctools": "^3.4",
        "drupal/job_scheduler": "^3.0",
        "flow/jsonpath": "~0.3",
        "drupal/token": "^1.7",
        "drupal/views_bulk_operations": "^3.8",
        "drupal/webform": "^5.12",
        "easyrdf/easyrdf": "^0.9.1",
        "drupal/cer": "^4.0",
        "drupal/panels": "^4.6",
        "drupal/page_manager": "^4.0",
        "drupal/views_bulk_edit": "^2.4",
        "drupal/views_tree": "^2.0",
        "drupal/superfish": "^1.4",
        "drupal/visitors": "^2.7",
        "drupal/login_history": "^1.0",
        "drupal/admin_toolbar": "^2.4",
        "drupal/pathauto": "^1.8",
        "drupal/disable_messages": "^2.0",
        "drupal/views_taxonomy_term_name_depth": "^7.0",
        "drupal/search_api": "^1.18",
        "drupal/search_api_autocomplete": "^1.4",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/redirect": "^1.6",
        "drupal/backup_migrate": "^4.1",
        "drupal/tfa": "^1.0@alpha",
        "drupal/ga_login": "^1.0@alpha",
        "drupal/services": "^4.0@beta",
        "drupal/real_aes": "^2.3",
        "drupal/paragraphs": "^1.12",
        "drupal/viewsreference": "^1.7",
        "drupal/features": "^3.12",
        "drupal/noreqnewpass": "^1.2",
        "drupal/pdf": "^1.1",
        "pear/archive_tar": "1.4.14"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "autoloader-suffix": "Drupal8"
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ]
}
